<launch>

  <arg name="db" default="false" />
  <arg name="db_path" default="$(find ur3_man_moveit_config)/default_warehouse_mongo_db" />
  <arg name="debug" default="false" />
  <arg name="use_gui" default="false" />

  <!-- Load the URDF, SRDF and other .yaml configuration files on the param server -->
  <include file="$(find ur3_man_moveit_config)/launch/planning_context.launch">
    <arg name="load_robot_description" value="true"/>
  </include>


 <!--  <node name="left_arm_pid_joint_trajectory_action_server" pkg="ur3_man_bringup" type="pid_traj_action_server.py" output="screen" respawn="false">
    <param name="driver_name" value="left_arm_ur_driver" />
    <param name="controller_name" value="left_arm_controller" />
  </node>

  <node name="right_arm_pid_joint_trajectory_action_server" pkg="ur3_man_bringup" type="pid_traj_action_server.py" output="screen" respawn="false">
    <param name="driver_name" value="right_arm_ur_driver" />
    <param name="controller_name" value="right_arm_controller" />
  </node> -->

  <!-- Run the main MoveIt! executable without trajectory execution (we do not have controllers configured by default) -->
  <include file="$(find ur3_man_moveit_config)/launch/move_group.launch">
    <arg name="allow_trajectory_execution" value="true"/>
    <arg name="fake_execution" value="false"/>
    <arg name="info" value="true"/>
    <arg name="debug" value="$(arg debug)"/>
  </include>

  <!-- Run Rviz and load the default config to see the state of the move_group node -->
  <include file="$(find ur3_man_moveit_config)/launch/moveit_rviz.launch">
    <arg name="config" value="true"/>
    <arg name="debug" value="$(arg debug)"/>
  </include>

  <!-- If database loading was enabled, start mongodb as well -->
  <include file="$(find ur3_man_moveit_config)/launch/default_warehouse_db.launch" if="$(arg db)">
    <arg name="moveit_warehouse_database_path" value="$(arg db_path)"/>
  </include>

</launch>
